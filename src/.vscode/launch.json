{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in library 'sewer56_archives_r3a'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--lib",
                    "--package=sewer56-archives-r3a"
                ],
                "filter": {
                    "name": "sewer56_archives_r3a",
                    "kind": "lib"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug benchmark 'my_benchmark'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--bench=my_benchmark",
                    "--package=sewer56-archives-r3a"
                ],
                "filter": {
                    "name": "my_benchmark",
                    "kind": "bench"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'r3a-dictionary-tester'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=r3a-dictionary-tester",
                    "--package=r3a-dictionary-tester"
                ],
                "filter": {
                    "name": "r3a-dictionary-tester",
                    "kind": "bin"
                }
            },
            "args": [ "--input", "/home/sewer/Downloads/smim", "--dict-size", "65536", "--no-solid-blocks"],
            "cwd": "${workspaceFolder}/research"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in executable 'r3a-dictionary-tester'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--bin=r3a-dictionary-tester",
                    "--package=r3a-dictionary-tester"
                ],
                "filter": {
                    "name": "r3a-dictionary-tester",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}/research"
        }
    ]
}
